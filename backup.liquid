            aspect-ratio: 1/1;
            height: 1rem;
            display: flex;
            justify-content: center;
            align-items: center;
  
            & .choose__header__figure{
              width: 0.5rem;
              width: 100%;
              aspect-ratio: 1/1;
              height: 0.5rem;
              height: 100%;
  
              & .choose__header__img{
                display: block;
                width: 100%;
                object-fit: cover;
                object-position: center;
                height: auto;
                transform: rotateZ(0);
                transition: all 0.3s ease;
              }
            }
          }
        }
      }

      &.choose__header--active{
        & .choose__header__container {
          & .choose__header__btn{
            & .choose__header__figure{
              & .choose__header__img{
                transform: rotateZ(-180deg);
              }
            }
          }
        }
      }
    }

    & .choose__accesory__options{
      padding-block: 1rem;
      display: grid;
      grid-template-rows: 0fr;
      transition: grid-template-rows 0.3s ease;

      & .choose__accesory__options__container {
        overflow: hidden;

        & .choose__accesory__options__content{
          display: flex;
          flex-direction: column;
          gap: 1rem;

          & .accesory__card__content{
            width: 100%;
            display: flex;
            flex-direction: column;
            align-items: center;
            gap: 2rem;

            & .accesory__card__material__element{
              padding: 1rem;
              border-radius: 1rem;
              display: flex;
              flex-direction: column;
              align-items: center;
              gap: 0.75rem;
              cursor: pointer;
              min-width: 200px;
              width: 50%;
              max-width: 380px;
              border: thin solid transparent;

              & .accesory__material__span{
 
                font-weight: 600;
              }

              &.accesory__element--selected{
                background-color:  #D6E7EF;
                border-color: var(--color-secundario);
              }
            }
          }

          & .accesory__options {
            width: 100%;
            display: flex;
            flex-direction: column;
            gap: 2rem;

            & .accesory__card__color {
              padding-bottom: 1rem;
              overflow-x: auto;
              
              & .accesory__card__container{
                min-width: 45rem;
                width: 100%;
                display: flex;
                gap: 0.5rem;
  
                & .accesory__card__color__content{
                  padding: 0.5rem 0.25rem;
                  background-color: #e7f5ff;
                  border: thin solid transparent;
                  border-radius: 0.5rem;
                  cursor: pointer;
                  flex-grow: 1;
                  
                  &.accesory__element--selected{
                    border-color: var(--color-secundario);
                  }
  
                  & .accesory__card__element{
                    display: flex;
                    flex-direction: column;
                    align-items: center;
                    text-align: center;
                    width: 100%;
                    gap: 0.5rem;
  
                    & span{
                      font-size: 0.7rem;
                    
                    }

                    & .color__icon__container{
                      width: 100%;
                      height: 100%;
                      display: flex;
                      justify-content: center;
                      align-items: center;
                      flex-grow: 1;
                    }

                    & .color__icon{
                      background-color: var(--color-card);
                    }
  
                    & .color__icon--white{
                      color: #fff;
                    }
                  }
                }
              }
            }

            & .group {
              display: none;
              opacity: 0;
              transition: all 0.3s ease;

              &.element--show{
                display: block;
                opacity: 1;
              }
            }
          }
        }
      }
    }

    & .choose__accesory__content:has(.choose__header--active) .choose__accesory__options{
      grid-template-rows: 1fr;
    }

    & .accesory__type__btns{
      display: flex;
      flex-direction: column;
      align-items: center;
      gap: 1rem;

      & .accesory__btn{
        background-image: linear-gradient(to right, var(--color-principal) 30%, var(--color-secundario));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
        color: transparent;
        font-weight: 600;
        border: none;
        outline: none;
        cursor: pointer;
        font-family: inherit;
        transition: all 300ms ease;
  
        padding: 0.75rem 4rem;
        display: block;
        border-radius: 2rem;

        &.action__btn--selected{
          -webkit-background-clip: unset;
          -webkit-text-fill-color: #fff;
          background-clip: unset;
          color: #fff;
        }
      }
    }

  }

  /*********** FORM-4 ***********/
  & .options__step__4{
    display: flex;
    flex-direction: column;
    gap: 2rem;

    & .form__article__extra{
      display: flex;
      flex-direction: column;
      gap: 2rem;

      & .form__extra{
        display: flex;
        flex-direction: column;
        gap: 2rem;
      }

      & .form__extra__title,
      & .heater__title{
        display: flex;
        align-items: center;
        gap: 1rem;
      
      }

      & .form__extra__btns{
        display: flex;
        width: 100%;
      }

      & .form__extra__btns__container{
        display: flex;
        flex-direction: column;
        width: 100%;
        gap: 0.5rem;
      }

      & .btn__extra {
        padding: 0.75rem;
        width: 90%;
      }

      & .form__extra__container {
        display: flex;
        flex-direction: column;
        gap: 1.5rem;
      }

      & .heater__container{
        display: flex;
        flex-direction: column;
        gap: 1rem;
      }

      & .header__input__container{
        background-color: #ecf7ff;
        padding: 0.75rem;
        border-radius: 1rem;
      }

      & .heater__input{
        display: flex;
        flex-direction: column;
        gap: 0.5rem;
      }

      & .heater__label{
        font-size: 0.8rem;
 
      }

      & .heater__input__count{
        text-align: center;
        border-bottom: thin solid var(--color-principal);
      }

      & .form__extra__heaters__alert{
        background-color: #ecddff;
        border: thin solid #8f33ff;
        border-radius: 1rem;
        padding: 0.75rem;
      }

      & .heaters__alert__container {
        display: flex;
        flex-direction: column;
        gap: 0.5rem;
      }

      & .alert__paragraph {
   
        font-size: 0.8rem;
      }

      & .alert__svg svg{
        fill: #8f33ff;
      }
      
    }
  }

  /*********** FORM-5 ***********/
  & .options__step__5{

    & .resume__container{
      display: flex;
      flex-direction: column;
      gap: 2.5rem;
    }

    & .form__resume__content{
      display: flex;
      flex-direction: column;
      gap: 1rem;
    }

    & .resume__title{
 
    }

    & .resume__inputs{
      display: flex;
      flex-direction: column;
      gap: 1.5rem;
    }

    & .resume__input{
      display: flex;
      flex-direction: column;
      gap: 0.5rem;
    }

    & .resume__label{
      font-size: 0.8rem;
 
    }

    & .form__input{
      padding: 1rem;
  
      text-align: start;
      background-color: #EDF5F9;
      border-radius: 0.5rem;
      border: thin solid var(--color-secundario);
    }

    & .inputs__shipping__container{
      display: flex;
      flex-direction: column;
      gap: 1.5rem;
    }

    & .form__input__additional__information{
      resize: none;
      min-height: 8rem;
    }

    & .resume__checkbox{
      display: flex;
      gap: 0.5rem;
    }

    & .label__form{
      font-size: 0.8rem;
      padding-bottom: 0;
      background-image: none;
      -webkit-background-clip: unset;
      -webkit-text-fill-color: unset;
      color: #000;
      user-select: none;
      position: relative;
      width: max-content;

      &::after{
        position: absolute;
        right: -0.75rem;
        top: -0.25rem;
        content: "*";
        font-size: 1.5rem;
        color: #c76973;
      }
    }

    & .link__terms{
      color: var(--color-secundario);
      text-decoration: underline;
      display: inline-block;
    }
  }

  & .alert__message{
    position: fixed;
    inset-inline: 0;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
    z-index: 5;
  }
  
  & .alert__message__container{
    position: absolute;
    top: 6rem;
    right: -100%;
    margin-inline: auto;
    border-top: medium solid #721c24;
    background-color: #f8d7da;
    color: #721c24;
    padding: 1rem;
 
    border-radius: 1rem;
    text-align: center;
    width: 90%;
    max-width: max-content;
    transition: all 0.3s ease;
  }

  & .alert__message.alert__message--show{

    & .alert__message__container{
      right: 0.5rem
    }
    /* animation: fadeOut 3s forwards; */
  }

  & .confirmation__container{
    position: fixed;
    inset-inline: 0;
    z-index: 5;
    top: 0;
    font-size: 0.85rem;
    display: flex;
    justify-content: center;
  }
  
  & .confirmation__container.confirmation--show{
    & .confirmation__title{
      top: 1rem;
    }
  }
  
  & .confirmation__title{
    position: absolute;
    z-index: 5;
    width: 80%;
    margin-inline: auto;
    max-width: max-content;
    font-family: var(--font-family-gilroy);
    overflow: hidden;
    background-color: #d4edda;
    color: #fff;
    border-radius: 1rem;
    border-top: medium solid #58b56d;
    top: -4rem;
    transition: top 1s ease, right 1s ease;

    & h3{
      font-weight: 400;
      padding: 1rem;
      color: #295233;
      text-align: center;
    }
  }
}


/***************************** MEDIA QUERIES *****************************/
@media screen and (width >= 768px) {
  #{{ section.id }}{
    & .status__bar__principal{
      overflow: hidden;
    }
    & .choose__header__container{
      padding: 1rem 0.5rem !important;
    }

    & .election__btns{
      flex-direction: row;
      max-width: unset;
      width: unset;
    }
  
    & .btn__election{
      padding: 0.75rem 4rem;
    }
  
    & .btns__footer__content{
      flex-direction: row;
      gap: 1rem;
    }

    & .options__step__4{

      & .form__article__extra{
        gap: 4rem;

        & .form__extra__btns__container{
          flex-direction: row;
        }

        & .form__extra__container.form__extra__long{
          flex-direction: row;
          align-items: center;
        }

        & .form__extra__heaters{
          flex-grow: 2;
          width: 0;
        }

        & .form__extra__heaters__alert{
          flex-grow: 1;
          width: 0;
        }
      }
    }

    /* & .alert__message{
      bottom: 6.5rem;
    } */

    & .options__step__5{
      & .personal__information{
        & .resume__inputs{
          display: grid;
          grid-template-columns: repeat(2, 1fr);
        }
      }

      & .shipping__address{
        & .inputs__shipping__container{
          flex-direction: row;

          & .resume__input{
            width: unset;
            flex-grow: 1;
          }
        }        
      }
    }

    & .confirmation__container.confirmation--show{
      & .confirmation__title{
        right: 0.5rem;
        top: 4rem;
      }
    }

    & .confirmation__title{
      top: 4rem;
      right: -100%;
    }
  }
}

@media screen and (width >= 1024px){
  #{{ section.id }}{
    & .container{
      width: 80%;
    }
  
    & .section__form{
      padding: 2rem 4rem;
    }
    & .alert__message{
      top:5rem;
    }
    & .form__article:first-child{
      flex-direction: row-reverse;
  
      & > *{
        flex-grow: 1;
      }
    }
  
    & .scene__table__container{
      width: 100%;
    }
  
    & .table{
      width: 80%;
      height: 80%;
      margin: auto;
    }
  
    & .attached__content{
      flex-direction: row-reverse;
      width: 100%;
  
      & > *{
        flex-grow: 1;
        width: 100%;
      }
    }
  
    & .attached__measures__container{
      width: max-content;
      height: 70%;
      display: flex;
      justify-content: center;
      align-items: center;
      margin: auto;
    }
  
    & .attached__list{
      gap: 0.5rem;
    }
  
    & .attached__measure__content{
      padding: 0.5rem 1rem;
    }
  
    & .scene__table__container{
      justify-content: flex-start;
      justify-content: center;
  
      & .scene__table{
        margin: 0;
      }
    }
  
    & .article__color__base:first-child{
      flex-direction: column;
    }

    & .options__step__3.options__table.table__material__color{

      & .scene__table__accesory{
        padding-bottom: 0;
      }

      & .choose__accesory__options{

        & .choose__accesory__options__content{
  
          & .accesory__material {
            
            & .accesory__card__content{
              flex-direction: row;

              & .accesory__card__material__element{
                min-width: unset;
              }
            }
          }

          & .accesory__type{

            & .accesory__type__content {

              & .accesory__type__btns{
                flex-direction: row;
                width: 80%;
                margin-inline: auto;

                & .accesory__btn {
                  width: 100%;
                }
              }
            }
          }
        }
      }
    }
  }
}

