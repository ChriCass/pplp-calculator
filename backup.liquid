  /***************** UTILITIES *****************/
.container{
  width: 95%;
  max-width: 75rem;
  max-width: 64rem;
  margin-inline: auto;
  overflow: hidden;
}

.flex-c-c{
  display: flex;
  justify-content: center;
  align-items: center;
}

.section__section__calculator__form{
  width: 100%;
  padding-block: 1rem;
}

.action__btn{
  background-image: linear-gradient(to right, var(--color-principal) 30%, var(--color-secundario));
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  color: transparent;
  font-weight: 400;
  background-color: #fff;
}

.action__btn--selected{
  border: none;
  color: #fff;
  background-color: transparent;
  background-clip: unset;
  -webkit-text-fill-color: #fff;
}

/***************** KEYFRAMES *****************/
@keyframes length{
  0%{
    transform: scaleX(1);
  }
  50%{
    transform: scaleX(1.2);
  }
}

@keyframes depth{
  0%{
    transform: scaleY(1);
  }
  50%{
    transform: scaleY(1.2);
  }
}

@keyframes height{
  0%{
    transform: scaleY(1);
  }
  50%{
    transform: scaleY(1.1);
  }
}

@keyframes rotate-circle{
  0%{
    transform: rotate(0deg);
  }
  100%{
    transform: rotate(360deg);
  }
}

@keyframes fadeOut {
  0% {
    opacity: 1;
  }
  100% {
    opacity: 0;
  }
}

/***************** PAGE *****************/
#{{ section.id }} h2{
  font-size: 26px !important;
}
#{{ section.id }} .alert__title__text{

font-size: 20px !important;

}
#{{ section.id }}{

  & .section__calculator__content{
    display: flex;
    flex-direction: column;
    gap: 2rem;
  }

  & .section__form.section__form--1,
  & .section__form.section__form--2,
  & .section__form.section__form--3,
  & .section__form.section__form--4,
  & .section__form.section__form--5{
    opacity: 0;
    display: none;
  }

  &:has(.section__calculator__content.step-1){
    & .section__calculator__content.step-1{
      & .status__bar.status__step-1::after{
        width: 15%;
      }
    }

    & .section__form--1{
      opacity: 1;
      display: block;

      @starting-style{
        opacity: 0;
      }
    }
  }

  &:has(.section__calculator__content.step-2){
    & .section__calculator__content.step-2{
      & .status__bar.status__step-1::after{
        width: 40%;
      }

      & .section__form--2{
        display: block;
        opacity: 1;

        @starting-style{
          opacity: 0;
        }
      }
    }
  }

  &:has(.section__calculator__content.step-3){
    & .section__calculator__content.step-3{
      & .status__bar.status__step-1::after{
        width: 65%;
      }

      & .section__form--3{
        display: block;
        opacity: 1;

        @starting-style{
          opacity: 0;
        }
      }
    }
  }

  &:has(.section__calculator__content.step-4){
    & .section__calculator__content.step-4{
      .status__bar.status__step-1::after{
        width: 90%;
      }

      & .section__form--4{
        display: block;
        opacity: 1;

        @starting-style{
          opacity: 0;
        }
      }
    }
  }

  &:has(.section__calculator__content.step-5){
    & .section__calculator__content.step-5{
      & .status__bar.status__step-1::after{
        width: 100%;
      }

      & .section__form--5{
        display: block;
        opacity: 1;

        @starting-style{
          opacity: 0;
        }
      }
    }
  }

  /*********** STATUS-BAR ***********/
  & .status__bar__principal{
    padding-block: 1rem;
    overflow: auto;
  
    &.container{
      width: 95%;
    }
  }
  
  & .status__bar__container{
    width: 100%;
    min-width: 45rem;
    display: flex;
    justify-content: space-between;
    position: relative;
  
    &.container{
      width: 95%;
      max-width: 75rem;
    }
  }
  
  & .status__content{
    display: flex;
    flex-direction: column;
    align-items: center;
    gap: 0.5rem;
    font-size: 0.8rem;
  }
  
  & .status__text{
    background-image: linear-gradient(to right, var(--color-principal) 30%, var(--color-secundario));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    color: transparent;
    font-weight: 600;
  
  }
  
  & .status__count{
    position: relative;
    width: 1.5rem;
    height: 1.5rem;
    aspect-ratio: 1/1;
    padding: 0.2rem;
    border: thin solid var(--color-secundario);
    background-color: #fff;
    border-radius: 50%;
    z-index: 2;
  }
  
  & .status__bar{
    position: absolute;
    bottom: 20%;
    inset-inline: 0;
    width: 92%;
    height: 6px;
    margin-left: 3.15rem;
    border: thin solid var(--color-principal);
    border-left: none;
    border-right: none;
    z-index: 1;
    transition: all 0.3s ease;
  
    &.status__step-1::after{
      transition: all 0.3s ease;
      content: "";
      position: absolute;
      height: 100%;
      width: calc(100% / 8);
      background-image: linear-gradient(to right, var(--color-principal) 30%, var(--color-secundario));
    }
  }
  
  & .status__content--last .status__count::after{
    content: "";
    position: absolute;
    left: 30.8px;
    width: 1rem;
    height: 1rem;
    background-color: #fff;
    z-index: 5;
  }
  
  & .status__content--active .status__count{
    border-color: var(--color-principal);
    color: #fff;
    background-color: var(--color-principal);
 
  }

  /*********** FORM ***********/
  & .design__content{
    overflow: hidden;
    width: 100%;
    margin: auto;
    border-radius: 1rem;
    background-image: linear-gradient(to right, var(--color-principal) 30%, var(--color-secundario));
  
  }
  
  & .form__design{
    margin: 1px;
    background-color: #fff;
    border-radius: 1rem;
    position: relative;
  }
  
  & .section__form{
    padding: 1rem;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    transition: height 0.3s ease, opacity 0.5s ease;
    interpolate-size: allow-keywords;
  }
  
  & .form__title{
    display: flex;
    justify-content: center;
    align-items: center;
    gap: 1rem;
    padding-block: 1rem;
  
