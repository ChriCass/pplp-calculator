  }
  
  & .contact__title__text{
    font-weight: 400;
    font-size: 21px !important;
  }
  
  & .contact__btn {
    display: flex;

    & .btn__contact{
      padding: 1rem;
      border-radius: 1rem;
  
      text-decoration: none;
      transition: 300ms ease;
      display: inline-block;
    
    } 
