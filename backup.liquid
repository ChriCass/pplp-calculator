    flex-direction: column;
    overflow: hidden;
  }
  
  & .form__article{
    padding-block: 1rem;
    overflow: hidden;
    display: flex;
    flex-direction: column;
    transition: height 0.3s ease;
    interpolate-size: allow-keywords;
    height: auto;
  
    & > * {
      transition: all 0.3s ease;
    }
  
    &:nth-child(2) {
      align-items: center;
    }
  }
  
  & .scene__table{
    width: 100%;
    height: 100%;
    aspect-ratio: 1/1;
    margin: auto;
    max-width: 20rem;
    max-height: 20rem;
    position: relative;
  }
  
  & .table{
    position: relative;
    width: 100%;
    aspect-ratio: 1/1;
    height: 100%;
    margin-inline: auto;
    transform: rotateX(0deg) rotateY(0deg);
    &:has(.edge__top:hover) .edge__top,
    &:has(.edge__top:hover) .edge__bottom,
    &:has(.edge__bottom:hover) .edge__top,
    &:has(.edge__bottom:hover) .edge__bottom,
    &:has(.edge__left:hover) .edge__left,
    &:has(.edge__left:hover) .edge__right,
    &:has(.edge__right:hover) .edge__left,
    &:has(.edge__right:hover) .edge__right{
      background-color: var(--color-secundario);
      box-shadow: 0 1px 16px 0 var(--color-secundario);
      z-index: 5;
    }
  }
  
  & .table__top{
    position: relative;
    width: 73.85%;
    height: 52.75%;
    background-color: transparent;
    transform: skewX(-45deg) rotateX(60deg) translateY(-50%) translateX(0);
    z-index: 5;
  
  }
  
  & .edge{
    background-color: #5558;
    border-radius: 1rem;
    transition: all 0.3s ease;
    position: absolute;
    cursor: pointer;
  }
  
  & .edge__top{
    top: 0;
    right: 0;
    width: 100%;
    height: 11px;
  }
  
  & .edge__right{
    top: 0;
    right: 0;
    height: 100%;
    width: 8px;
  }
  
  & .edge__bottom{
    bottom: 0;
    left: 0;
    width: 100%;
    height: 11px;
  }
  
  & .edge__left{
    left: 0;
    bottom: 0;
    width: 8px;
    height: 100%;
  }
  
  & .table__legs{
    position: absolute;
    inset: 0;
  
    &:has(.leg:hover) .leg{
      background-color: var(--color-secundario);
      box-shadow: 0 1px 16px 0 var(--color-secundario);
    }
  }
  
  & .leg {
    position: absolute;
    width: 6px;
    height: 70%;
    background-color: #5558;
    border-radius: 1rem;
    cursor: pointer;
    transition: all 0.3s ease;
  }
  
  & .leg__front__left {
    top: 25%;
    left: 0.8%;
  }
  
  & .leg__front__right {
    top: 25%;
    right: 25%;
  }
  
  & .leg__back__left {
    top: 3px;
    left: 26.5%;
  }
  
  & .leg__back__right {
    top: 0;
    right: 0;
    transform: translateZ(100px);
  }
  
  & .side__length--active,
  & .side__depth--active,
  & .side__height--active{
    background-color: var(--color-secundario);
    box-shadow: 0 1px 16px 0 var(--color-secundario);
    z-index: 5;
  }
  
  & .side__length--active{
    animation: length 1s ease infinite;
  }
  
  & .side__depth--active{
    animation: depth 1s ease infinite;
  }
  
  & .side__height--active{
    z-index: 2;
    animation: height 1s ease infinite;
    transform-origin: top;
  }
  
  & .form__info{
    display: flex;
    flex-direction: column;
    & > *{
      padding-block: 1rem;
    }
  }
  
  & .form__btns{
    display: flex;
