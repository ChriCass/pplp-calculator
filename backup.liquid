    flex-direction: column;
    overflow: hidden;
    border-bottom: thin solid transparent;
    transition: all 0.3s ease;
  
    &:has(.form__input:focus) {
      border-bottom-color: var(--color-secundario);
    }

    &:not(:has(.form__input:placeholder-shown)){
      border-bottom-color: var(--color-secundario);
    }
  }
  
  & .label__input{
    font-size: 0.8rem;
    cursor: pointer;
    padding: 0;
 
    background-clip: text;
    color: black;
    font-weight: 600;
    text-transform: none !important;
  }
  
  & .input__measure{
    display: flex;
    align-items: center;
    justify-content: center;
  
    & > *{
      flex-grow: 1;
      width: 0;
    }
  }
  
  & .form__input{
    padding: 0.5rem;
    text-align: center;
    text-align: end;
 

    background-color: transparent;
    border: none;
    outline: none;
    cursor: pointer;
    transition: 300ms ease;
    display: block;
    margin: 0; 
  }
  
  & .input__unit{
    text-align: start;
 
  }
  
  & .alert__container{
    display: flex;
    flex-direction: column;
    gap: 1rem;
    border-radius: 0.6rem;
    background-color: rgba(151, 71, 255, 0.08);
    border: thin solid #8f33ff;
    padding: 0.75rem;
    overflow: hidden;

    & .paragraph__text{
  
    }
  }
  
  & .alert__title{
    display: flex;
    align-items: center;
    gap: 1rem;

 
  }
  
  & .alert__svg__icon{
    background-color: #8f33ff;
