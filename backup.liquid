  & .form__article.form__footer.form__btns__footer.element--show,
  & .election__attached.element--show{
    display: block;
    opacity: 1;
    transition: opacity 0.3s ease;
  
    @starting-style{
      opacity: 0;
    }
  }

  & .form__article:last-child{
    display: flex;
    flex-direction: column;
    gap: 3rem;
  }
  
  & .form__btns__footer{
    width: 100%;
  }
  
  & .election__contact__container{
    width: 100%;
    margin-inline: auto;
    max-width: 416px;
    border-radius: 1rem;
    background-color: rgba(233, 240, 243, 1);
  }
  
  & .election__contact__content{
    overflow: hidden;
